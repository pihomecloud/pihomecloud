#{{ansible_managed}}
#Rule (1001) msg:double quote
#cookie: user=".*"
#zone0=HEADERS&id0=1001&var_name0=cookie
BasicRule  wl:1001 "mz:$HEADERS_VAR:Cookie";

#Rule (1302) "msg:html open tag"
#request: search=.*bp:<0
BasicRule  wl:1302 "mz:$ARGS_VAR:search";

#Rule (1303) "msg:html close tag"
#request: search=.*bp:>0
BasicRule  wl:1303 "mz:$ARGS_VAR:search";

#Rule (1015) ", in stuff"
#ARGS response_text=Svc: hostname/servicename, check forced
BasicRule  wl:1015 "mz:$ARGS_VAR:response_text";

#Rule (1315) "double encoding !"
#ARGS response_text=Svc: hostname/Etat%20du%20dernier%20backup, check forced
BasicRule  wl:1315 "mz:$ARGS_VAR:response_text";

